data: {"type":"hook_event","data":{"agentId":"josh.stuart-ORG101311-6a2483e4","sessionId":"session-mdfpn1ts-6d5rrc","hookType":"post_tool_use","timestamp":"2025-07-23T08:37:18.976Z","data":{"toolName":"Read","sessionId":"b3876849-5fd2-4373-b330-b687eac19d6f","transcriptPath":"/Users/josh.stuart/.claude/projects/-Users-josh-stuart-Workspace-Dev-claude-companion/b3876849-5fd2-4373-b330-b687eac19d6f.jsonl","cwd":"/Users/josh.stuart/Workspace/Dev/claude-companion","rawInput":{"session_id":"b3876849-5fd2-4373-b330-b687eac19d6f","transcript_path":"/Users/josh.stuart/.claude/projects/-Users-josh-stuart-Workspace-Dev-claude-companion/b3876849-5fd2-4373-b330-b687eac19d6f.jsonl","cwd":"/Users/josh.stuart/Workspace/Dev/claude-companion","hook_event_name":"PostToolUse","tool_name":"Read","tool_input":{"file_path":"/Users/josh.stuart/Workspace/Dev/claude-companion/client/src/components/SessionEventView.tsx"},"tool_response":{"type":"text","file":{"filePath":"/Users/josh.stuart/Workspace/Dev/claude-companion/client/src/components/SessionEventView.tsx","content":"import React, { useState, useEffect } from 'react';\nimport { EventControl } from './EventControl';\n\ninterface HookEvent {\n  agentId: string;\n  sessionId: string;\n  hookType: string;\n  timestamp: string;\n  data: any;\n}\n\ninterface SessionEventViewProps {\n  sessionId: string;\n  agentId?: string;\n}\n\nexport const SessionEventView: React.FC<SessionEventViewProps> = ({\n  sessionId,\n  agentId\n}) => {\n  const [events, setEvents] = useState<HookEvent[]>([]);\n  const [loading, setLoading] = useState(true);\n  const [sessionInfo, setSessionInfo] = useState<any>(null);\n\n  useEffect(() => {\n    if (sessionId) {\n      fetchSessionData();\n    }\n  }, [sessionId]);\n\n  const fetchSessionData = async () => {\n    try {\n      setLoading(true);\n      \n      // Fetch session info\n      const sessionResponse = await fetch(`/api/sessions/${sessionId}`);\n      const session = await sessionResponse.json();\n      setSessionInfo(session);\n      \n      // Fetch session events\n      const eventsResponse = await fetch(`/api/sessions/${sessionId}/events`);\n      const sessionEvents = await eventsResponse.json();\n      setEvents(sessionEvents);\n      \n      setLoading(false);\n    } catch (error) {\n      console.error('Failed to fetch session data:', error);\n      setLoading(false);\n    }\n  };\n\n  const formatTimestamp = (timestamp: string) => {\n    return new Date(timestamp).toLocaleTimeString('en-US', {\n      hour: '2-digit',\n      minute: '2-digit',\n      second: '2-digit'\n    });\n  };\n\n  const formatDuration = (startTime: string, endTime?: string) => {\n    const start = new Date(startTime);\n    const end = endTime ? new Date(endTime) : new Date();\n    const diffMs = end.getTime() - start.getTime();\n    const diffMins = Math.floor(diffMs / (1000 * 60));\n    const diffSecs = Math.floor((diffMs % (1000 * 60)) / 1000);\n    \n    if (diffMins > 0) {\n      return `${diffMins}m ${diffSecs}s`;\n    }\n    return `${diffSecs}s`;\n  };\n\n  if (loading) {\n    return (\n      <div className=\"flex-1 flex items-center justify-center\">\n        <div className=\"animate-spin rounded-full h-8 w-8 border-2 border-blue-500 border-t-transparent\"></div>\n      </div>\n    );\n  }\n\n  if (!sessionInfo) {\n    return (\n      <div className=\"flex-1 flex items-center justify-center text-gray-500\">\n        Session not found\n      </div>\n    );\n  }\n\n  return (\n    <div className=\"flex-1 bg-white overflow-hidden\">\n      {/* Session Header */}\n      <div className=\"border-b border-gray-200 bg-gray-50 px-6 py-4\">\n        <div className=\"flex items-center justify-between\">\n          <div>\n            <h1 className=\"text-xl font-semibold text-gray-900\">\n              {sessionInfo.name}\n            </h1>\n            <div className=\"mt-1 flex items-center space-x-4 text-sm text-gray-500\">\n              <span>\n                📋 Session: {sessionInfo.id.substring(0, 8)}...\n              </span>\n              <span>\n                🤖 Agent: {agentId?.substring(0, 12)}...\n              </span>\n              <span>\n                📁 {sessionInfo.workingDirectory.split('/').pop()}\n              </span>\n            </div>\n          </div>\n          <div className=\"text-right\">\n            <div className={`inline-flex items-center px-2.5 py-0.5 rounded-full text-xs font-medium ${\\n              sessionInfo.status === 'active'\\n                ? 'bg-green-100 text-green-800'\\n                : sessionInfo.status === 'completed'\\n                ? 'bg-gray-100 text-gray-800'\\n                : 'bg-red-100 text-red-800'\\n            }`}>\\n              {sessionInfo.status === 'active' && '🟢'}\\n              {sessionInfo.status === 'completed' && '✅'}\\n              {sessionInfo.status === 'interrupted' && '⚠️'}\\n              <span className=\\\"ml-1 capitalize\\\">{sessionInfo.status}</span>\\n            </div>\\n            <div className=\\\"mt-1 text-xs text-gray-500\\\">\\n              Duration: {formatDuration(sessionInfo.startTime, sessionInfo.endTime)}\\n            </div>\\n          </div>\\n        </div>\\n      </div>\\n\\n      {/* Events List */}\\n      <div className=\\\"flex-1 overflow-y-auto\\\">\\n        {events.length === 0 ? (\\n          <div className=\\\"text-center py-12 text-gray-500\\\">\\n            <div className=\\\"text-4xl mb-4\\\">📝</div>\\n            <h3 className=\\\"text-lg font-medium text-gray-900 mb-2\\\">No events yet</h3>\\n            <p>Events will appear here as the agent works</p>\\n          </div>\\n        ) : (\\n          <div className=\\\"space-y-4 p-6\\\">\\n            {events.map((event, index) => (\\n              <div key={`${event.sessionId}-${index}`} className=\\\"border border-gray-200 rounded-lg\\\">\\n                <div className=\\\"flex items-center justify-between p-4 border-b border-gray-100 bg-gray-50\\\">\\n                  <div className=\\\"flex items-center space-x-3\\\">\\n                    <div className=\\\"text-sm font-medium text-gray-900\\\">\\n                      {event.hookType.replace('_', ' ').toUpperCase()}\\n                    </div>\\n                    {event.data?.toolName && (\\n                      <div className=\\\"text-sm text-gray-500\\\">\\n                        Tool: <span className=\\\"font-mono\\\">{event.data.toolName}</span>\\n                      </div>\\n                    )}\\n                  </div>\\n                  <div className=\\\"text-xs text-gray-500\\\">\\n                    {formatTimestamp(event.timestamp)}\\n                  </div>\\n                </div>\\n                \\n                <div className=\\\"p-4\\\">\\n                  <EventControl\\n                    event={event}\\n                    onApprove={(reason, feedback) => {\\n                      console.log('Approve:', reason, feedback);\\n                    }}\\n                    onDeny={(reason, feedback) => {\\n                      console.log('Deny:', reason, feedback);\\n                    }}\\n                    onInjectContext={(context) => {\\n                      console.log('Inject context:', context);\\n                    }}\\n                  />\\n                </div>\\n              </div>\\n            ))}\\n          </div>\\n        )}\\n      </div>\\n    </div>\\n  );\\n};","numLines":111,"startLine":1,"totalLines":111}}},"computerId":"computer-1352a238","computerName":"josh.stuart's ORG101311","hostname":"ORG101311","platform":"darwin","agentName":"claude-companion","workingDirectory":"/Users/josh.stuart/Workspace/Dev/claude-companion"}},"timestamp":"2025-07-23T08:37:18.986Z"}

