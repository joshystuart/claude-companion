data: {"type":"hook_event","data":{"agentId":"josh.stuart-ORG101311-6a2483e4","sessionId":"session-mdfpoq2e-2ag04z","hookType":"post_tool_use","timestamp":"2025-07-23T08:38:37.046Z","data":{"toolName":"Read","sessionId":"b3876849-5fd2-4373-b330-b687eac19d6f","transcriptPath":"/Users/josh.stuart/.claude/projects/-Users-josh-stuart-Workspace-Dev-claude-companion/b3876849-5fd2-4373-b330-b687eac19d6f.jsonl","cwd":"/Users/josh.stuart/Workspace/Dev/claude-companion","rawInput":{"session_id":"b3876849-5fd2-4373-b330-b687eac19d6f","transcript_path":"/Users/josh.stuart/.claude/projects/-Users-josh-stuart-Workspace-Dev-claude-companion/b3876849-5fd2-4373-b330-b687eac19d6f.jsonl","cwd":"/Users/josh.stuart/Workspace/Dev/claude-companion","hook_event_name":"PostToolUse","tool_name":"Read","tool_input":{"file_path":"/Users/josh.stuart/Workspace/Dev/claude-companion/client/src/components/CommandHistory.tsx"},"tool_response":{"type":"text","file":{"filePath":"/Users/josh.stuart/Workspace/Dev/claude-companion/client/src/components/CommandHistory.tsx","content":"import React, { useEffect, useState } from 'react';\nimport { RemoteCommand } from '../types';\nimport { commandsApi } from '../services/commands-api';\n\nexport const CommandHistory: React.FC = () => {\n  const [commands, setCommands] = useState<RemoteCommand[]>([]);\n  const [loading, setLoading] = useState(true);\n\n  useEffect(() => {\n    loadCommands();\n    // Refresh every 5 seconds\n    const interval = setInterval(loadCommands, 5000);\n    return () => clearInterval(interval);\n  }, []);\n\n  const loadCommands = async () => {\n    try {\n      const allCommands = await commandsApi.getAllCommands();\n      setCommands(allCommands.sort((a, b) => \n        new Date(b.createdAt).getTime() - new Date(a.createdAt).getTime()\n      ));\n    } catch (error) {\n      console.error('Failed to load commands:', error);\n    } finally {\n      setLoading(false);\n    }\n  };\n\n  const getStatusBadge = (status: RemoteCommand['status']) => {\n    const colors = {\n      pending: 'bg-yellow-100 text-yellow-800',\n      processing: 'bg-blue-100 text-blue-800',\n      completed: 'bg-green-100 text-green-800',\n      expired: 'bg-gray-100 text-gray-800',\n    };\n\n    return (\n      <span className={`px-2 py-1 text-xs font-medium rounded-full ${colors[status]}`}>\n        {status.toUpperCase()}\n      </span>\n    );\n  };\n\n  const getCommandIcon = (type: RemoteCommand['type']) => {\n    const icons = {\n      approve: '✅',\n      deny: '❌',\n      context: '💬',\n      continue: '▶️',\n      stop: '⏹️',\n      interrupt: '🛑',\n    };\n    return icons[type] || '❓';\n  };\n\n  if (loading) {\n    return <div className=\"text-center py-4\">Loading command history...</div>;\n  }\n\n  return (\n    <div className=\"bg-white rounded-lg shadow p-6\">\n      <h2 className=\"text-lg font-semibold mb-4\">Command History</h2>\n      \n      {commands.length === 0 ? (\n        <p className=\"text-gray-500 text-center py-4\">No commands sent yet</p>\n      ) : (\n        <div className=\"space-y-3\">\n          {commands.map((command) => (\n            <div key={command.id} className=\"border border-gray-200 rounded-lg p-3\">\n              <div className=\"flex justify-between items-start mb-2\">\n                <div className=\"flex items-center gap-2\">\n                  <span className=\"text-lg\">{getCommandIcon(command.type)}</span>\n                  <span className=\"font-medium\">{command.type}</span>\n                  {getStatusBadge(command.status)}\n                </div>\n                <div className=\"text-xs text-gray-500\">\n                  {new Date(command.createdAt).toLocaleString()}\n                </div>\n              </div>\n              \n              <div className=\"text-sm text-gray-600\">\n                <div>Agent: {command.agentId}</div>\n                {command.payload.reason && (\n                  <div className=\"mt-1\">Reason: {command.payload.reason}</div>\n                )}\n                {command.payload.feedback && (\n                  <div className=\"mt-1\">Feedback: {command.payload.feedback}</div>\n                )}\n                {command.payload.instructions && (\n                  <div className=\"mt-1\">Instructions: {command.payload.instructions}</div>\n                )}\n              </div>\n              \n              {command.status === 'pending' && (\n                <div className=\"mt-2 text-xs text-orange-600\">\n                  Expires: {new Date(command.expiresAt).toLocaleTimeString()}\n                </div>\n              )}\n            </div>\n          ))}\n        </div>\n      )}\n    </div>\n  );\n};","numLines":105,"startLine":1,"totalLines":105}}},"computerId":"computer-1352a238","computerName":"josh.stuart's ORG101311","hostname":"ORG101311","platform":"darwin","agentName":"claude-companion","workingDirectory":"/Users/josh.stuart/Workspace/Dev/claude-companion"}},"timestamp":"2025-07-23T08:38:37.058Z"}

