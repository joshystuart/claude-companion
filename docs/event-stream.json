data: {"type":"hook_event","data":{"agentId":"josh.stuart-ORG101311-6a2483e4","sessionId":"session-mdfpoq6f-bkockt","hookType":"post_tool_use","timestamp":"2025-07-23T08:38:37.191Z","data":{"toolName":"Read","sessionId":"b3876849-5fd2-4373-b330-b687eac19d6f","transcriptPath":"/Users/josh.stuart/.claude/projects/-Users-josh-stuart-Workspace-Dev-claude-companion/b3876849-5fd2-4373-b330-b687eac19d6f.jsonl","cwd":"/Users/josh.stuart/Workspace/Dev/claude-companion","rawInput":{"session_id":"b3876849-5fd2-4373-b330-b687eac19d6f","transcript_path":"/Users/josh.stuart/.claude/projects/-Users-josh-stuart-Workspace-Dev-claude-companion/b3876849-5fd2-4373-b330-b687eac19d6f.jsonl","cwd":"/Users/josh.stuart/Workspace/Dev/claude-companion","hook_event_name":"PostToolUse","tool_name":"Read","tool_input":{"file_path":"/Users/josh.stuart/Workspace/Dev/claude-companion/client/src/components/SessionControl.tsx"},"tool_response":{"type":"text","file":{"filePath":"/Users/josh.stuart/Workspace/Dev/claude-companion/client/src/components/SessionControl.tsx","content":"import React, { useState } from 'react';\nimport { Agent } from '../types';\nimport { commandsApi } from '../services/commands-api';\n\ninterface SessionControlProps {\n  agent: Agent;\n  onCommandSent?: () => void;\n}\n\nexport const SessionControl: React.FC<SessionControlProps> = ({ agent, onCommandSent }) => {\n  const [instructions, setInstructions] = useState('');\n  const [isSubmitting, setIsSubmitting] = useState(false);\n\n  const handleContinue = async () => {\n    setIsSubmitting(true);\n    try {\n      await commandsApi.continueSession(\n        agent.id,\n        agent.sessionId || '',\n        instructions || 'Continue with the task'\n      );\n      onCommandSent?.();\n      setInstructions('');\n    } catch (error) {\n      console.error('Failed to continue session:', error);\n    } finally {\n      setIsSubmitting(false);\n    }\n  };\n\n  const handleStop = async () => {\n    setIsSubmitting(true);\n    try {\n      await commandsApi.stopSession(\n        agent.id,\n        agent.sessionId || '',\n        'Stopped by dashboard'\n      );\n      onCommandSent?.();\n    } catch (error) {\n      console.error('Failed to stop session:', error);\n    } finally {\n      setIsSubmitting(false);\n    }\n  };\n\n  const handleInterrupt = async () => {\n    setIsSubmitting(true);\n    try {\n      await commandsApi.interruptSession(\n        agent.id,\n        agent.sessionId || '',\n        'Emergency interrupt from dashboard'\n      );\n      onCommandSent?.();\n    } catch (error) {\n      console.error('Failed to interrupt session:', error);\n    } finally {\n      setIsSubmitting(false);\n    }\n  };\n\n  const handleInjectContext = async () => {\n    if (!instructions) return;\n    \n    setIsSubmitting(true);\n    try {\n      await commandsApi.injectContext(\n        agent.id,\n        agent.sessionId || '',\n        instructions\n      );\n      onCommandSent?.();\n      setInstructions('');\n    } catch (error) {\n      console.error('Failed to inject context:', error);\n    } finally {\n      setIsSubmitting(false);\n    }\n  };\n\n  return (\n    <div className=\"bg-white rounded-lg shadow p-6\">\n      <h3 className=\"text-lg font-semibold mb-4\">Session Control - {agent.id}</h3>\n      \n      <div className=\"mb-4\">\n        <div className=\"flex items-center gap-2 mb-2\">\n          <span className={`w-2 h-2 rounded-full ${\n            agent.status === 'active' ? 'bg-green-500' : \n            agent.status === 'idle' ? 'bg-yellow-500' : 'bg-gray-500'\n          }`} />\n          <span className=\"text-sm font-medium\">\n            Status: {agent.status}\n          </span>\n        </div>\n        {agent.sessionId && (\n          <div className=\"text-xs text-gray-500\">\n            Session: {agent.sessionId}\n          </div>\n        )}\n      </div>\n\n      <div className=\"space-y-4\">\n        {/* Emergency Interrupt Button - Prominent placement */}\n        <div className=\"border-2 border-red-200 rounded-lg p-3 bg-red-50\">\n          <div className=\"flex items-center justify-between mb-2\">\n            <span className=\"text-sm font-medium text-red-800\">Emergency Control</span>\n            <span className=\"text-xs text-red-600\">⚠️ Immediate action</span>\n          </div>\n          <button\n            onClick={handleInterrupt}\n            className=\"w-full px-4 py-3 bg-red-600 text-white rounded-lg hover:bg-red-700 transition-colors disabled:opacity-50 font-medium text-sm\"\n            disabled={isSubmitting || agent.status !== 'active'}\n          >\n            🛑 INTERRUPT CLAUDE (ESC)\n          </button>\n          <p className=\"text-xs text-red-600 mt-1\">\n            Immediately stops Claude's current execution, like pressing ESC in CLI\n          </p>\n        </div>\n\n        <div>\n          <label className=\"block text-sm font-medium text-gray-700 mb-2\">\n            Instructions / Context\n          </label>\n          <textarea\n            value={instructions}\n            onChange={(e) => setInstructions(e.target.value)}\n            placeholder=\"Provide instructions or context for Claude...\"\n            className=\"w-full px-3 py-2 border border-gray-300 rounded-md text-sm\"\n            rows={4}\n            disabled={isSubmitting}\n          />\n        </div>\n\n        <div className=\"grid grid-cols-2 gap-2\">\n          <button\n            onClick={handleContinue}\n            className=\"px-4 py-2 bg-green-500 text-white rounded hover:bg-green-600 transition-colors disabled:opacity-50\"\n            disabled={isSubmitting || agent.status !== 'active'}\n          >\n            Continue Session\n          </button>\n          <button\n            onClick={handleStop}\n            className=\"px-4 py-2 bg-red-500 text-white rounded hover:bg-red-600 transition-colors disabled:opacity-50\"\n            disabled={isSubmitting || agent.status !== 'active'}\n          >\n            Stop Session\n          </button>\n        </div>\n\n        {instructions && (\n          <button\n            onClick={handleInjectContext}\n            className=\"w-full px-4 py-2 bg-blue-500 text-white rounded hover:bg-blue-600 transition-colors disabled:opacity-50\"\n            disabled={isSubmitting}\n          >\n            Inject Context Only\n          </button>\n        )}\n      </div>\n\n      <div className=\"mt-4 p-3 bg-gray-50 rounded text-xs text-gray-600\">\n        <p className=\"font-medium mb-1\">How Session Control Works:</p>\n        <ul className=\"list-disc list-inside space-y-1\">\n          <li><strong>Interrupt</strong>: Immediately stops execution (like ESC in CLI)</li>\n          <li><strong>Continue</strong>: Sends instructions when Claude tries to stop</li>\n          <li><strong>Stop</strong>: Forces Claude to end the current session</li>\n          <li><strong>Inject Context</strong>: Sends guidance on the next tool use</li>\n        </ul>\n      </div>\n    </div>\n  );\n};","numLines":175,"startLine":1,"totalLines":175}}},"computerId":"computer-1352a238","computerName":"josh.stuart's ORG101311","hostname":"ORG101311","platform":"darwin","agentName":"claude-companion","workingDirectory":"/Users/josh.stuart/Workspace/Dev/claude-companion"}},"timestamp":"2025-07-23T08:38:37.201Z"}

